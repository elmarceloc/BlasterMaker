{
  "name": "BlasterMaker",
  "version": "0.7.3",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "gh-publish": "electron-builder --x64 -p always"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "custom-electron-titlebar": "^3.2.4",
    "dialogs": "^2.0.1",
    "electron-google-analytics": "^1.0.2",
    "electron-packager": "^15.2.0",
    "electron-reload": "^1.5.0",
    "electron-tooltip": "^1.1.5",
    "i": "^0.3.6",
    "image-size": "^0.9.3",
    "minimist": "^1.2.5",
    "npm": "^6.14.8",
    "open": "^7.3.0",
    "slugify": "^1.4.6",
    "sync-request": "^6.1.0",
    "universal-analytics": "^0.4.23"
  },
  "build": {
    "copyright": "Copyright © 2020 BlasterChile.cl",
    "productName": "Blaster Maker",
    "appId": "com.blasterchile.id",
    "mac": {
      "category": "public.app-category.graphics-design",
      "hardenedRuntime": true
    },
    "win": {
      "target": "nsis",
      "icon": "/icon.ico",
      "legalTrademarks": "Copyright © 2020 BlasterChile.cl"
    },
    "linux": {
      "target": "deb"
    },
    "fileAssociations": [{
      "ext": "bm",
      "name": "BlasterMaker",
      "role": "Editor",
      "icon": "../icon.ico"
    }]
  }
}
